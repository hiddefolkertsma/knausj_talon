app: firefox # assuming use of overleaf and firefox
app: Firefox
-
leaf cite:
    "\\cite{}"
    key(left)

leaf figure:
    "\\begin{figure"
    key(enter)

leaf section:
    "\\se"
    key(enter)

leaf subsection:
    "\\subse"
    key(enter)

leaf subsubsection:
    "\\subsubse"
    key(enter)

leaf use package:
    "\\usepackage{}"
    key(left)

leaf recompile:
    key(cmd-s)